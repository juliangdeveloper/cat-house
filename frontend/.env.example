# Example Environment Variables
# Copy this file to .env.development or .env.production and fill in the values
# All variables prefixed with EXPO_PUBLIC_ are accessible in client code

# API Configuration
EXPO_PUBLIC_API_URL=https://chapi.gamificator.click

# Environment
EXPO_PUBLIC_ENV=development

# Feature Flags (for future implementation)
EXPO_PUBLIC_OFFLINE_ENABLED=false
EXPO_PUBLIC_CAT_UI_LOADING_ENABLED=false

# Debug
EXPO_PUBLIC_DEBUG=true
